<h3>登録者数ランキング</h3>
<table>
  <tr>
    <th>順位</th>
    <th>チャンネル名</th>
    <th>登録者数</th>
  </tr>
  <%  @rank.each.with_index(1) do |data, i| %>
    <tr>
      <td><%= "第#{i}位" %></td>
      <td><%= link_to "#{data.name}", "/youtubers/#{data.youtuber_id}" %></td>
      <td><%= data.number_of_registrant %></td>
    </tr>
  <% end %>
</table>